<p id="notice"><%= notice %></p>

<h1>Raidition Infos</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Terrain reflectivity</th>
      <th>Point radius</th>
      <th>Latidude</th>
      <th>Logitude</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @raidition_infos.each do |raidition_info| %>
      <tr>
        <td><%= raidition_info.date %></td>
        <td><%= raidition_info.terrain_reflectivity %></td>
        <td><%= raidition_info.point_radius %></td>
        <td><%= raidition_info.latidude %></td>
        <td><%= raidition_info.logitude %></td>
        <td><%= raidition_info.city %></td>
        <td><%= link_to 'Show', raidition_info %></td>
        <td><%= link_to 'Edit', edit_raidition_info_path(raidition_info) %></td>
        <td><%= link_to 'Destroy', raidition_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Raidition Info', new_raidition_info_path %>
