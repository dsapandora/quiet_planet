<p id="notice"><%= notice %></p>

<h1>Polution Infos</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Vcdapstrat</th>
      <th>Vcdaptrop</th>
      <th>Amfstrat</th>
      <th>Latidude</th>
      <th>Logitude</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @polution_infos.each do |polution_info| %>
      <tr>
        <td><%= polution_info.date %></td>
        <td><%= polution_info.vcdapstrat %></td>
        <td><%= polution_info.vcdaptrop %></td>
        <td><%= polution_info.amfstrat %></td>
        <td><%= polution_info.latidude %></td>
        <td><%= polution_info.logitude %></td>
        <td><%= polution_info.city %></td>
        <td><%= link_to 'Show', polution_info %></td>
        <td><%= link_to 'Edit', edit_polution_info_path(polution_info) %></td>
        <td><%= link_to 'Destroy', polution_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Polution Info', new_polution_info_path %>
