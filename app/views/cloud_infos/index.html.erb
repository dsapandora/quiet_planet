<p id="notice"><%= notice %></p>

<h1>Cloud Infos</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Amfstrat</th>
      <th>Latidude</th>
      <th>Amftropclear</th>
      <th>Amftropcloudy</th>
      <th>Logitude</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cloud_infos.each do |cloud_info| %>
      <tr>
        <td><%= cloud_info.date %></td>
        <td><%= cloud_info.amfstrat %></td>
        <td><%= cloud_info.latidude %></td>
        <td><%= cloud_info.amftropclear %></td>
        <td><%= cloud_info.amftropcloudy %></td>
        <td><%= cloud_info.logitude %></td>
        <td><%= cloud_info.city %></td>
        <td><%= link_to 'Show', cloud_info %></td>
        <td><%= link_to 'Edit', edit_cloud_info_path(cloud_info) %></td>
        <td><%= link_to 'Destroy', cloud_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cloud Info', new_cloud_info_path %>
